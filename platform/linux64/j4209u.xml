<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project j4210u-app-linux">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <target name="create_run_jar">
        <jar destfile="./j4210u.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="jence.swt.app.UhfApp"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="./bin"/>
            <zipfileset excludes="META-INF/*.SF" src="./lib/swt.jar"/>
            <fileset dir="./lib"/>
            <fileset file="./libj4210u.so"/>
        </jar>
    </target>
</project>
